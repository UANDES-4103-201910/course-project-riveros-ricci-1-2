<p id="notice"><%= notice %></p>

<h1>Admin Geofences</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_geofences.each do |admin_geofence| %>
      <tr>
        <td><%= admin_geofence.user %></td>
        <td><%= admin_geofence.latitude %></td>
        <td><%= admin_geofence.longitude %></td>
        <td><%= link_to 'Show', admin_geofence %></td>
        <td><%= link_to 'Edit', edit_admin_geofence_path(admin_geofence) %></td>
        <td><%= link_to 'Destroy', admin_geofence, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Admin Geofence', new_admin_geofence_path %>
